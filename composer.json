{
    "name": "morenorafael/multitenant",
    "description": "Este paquete nos permite crear bases de datos y gestionarlas en un solo sistema, es muy util para sistemas SAAS como POS o helpdesk.",
    "authors": [
        {
            "name": "Rafael Moreno",
            "email": "alejandro8924@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "MorenoRafael\\MultiTenant\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MorenoRafael\\MultiTenant\\Providers\\MultiTenantServiceProvider"
            ],
            "aliases": {
                "MultiTenant": "MorenoRafael\\MultiTenant\\Facades\\Database"
            }
        }
    },
    "minimum-stability": "dev"
}
